-- Favori oyuncu yaklaştığında bildirim (GUI’de)
local function checkFavoriteProximity()
	if not favoritePlayer or not favoritePlayer.Character or not favoritePlayer.Character:FindFirstChild("HumanoidRootPart") then return end
	local localHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
	if not localHRP then return end

	local dist = (favoritePlayer.Character.HumanoidRootPart.Position - localHRP.Position).Magnitude
	if dist < 20 then
		favDropdown.BackgroundColor3 = Color3.fromRGB(255, 215, 0) -- Altın sarısı yanar
	else
		favDropdown.BackgroundColor3 = currentTheme == "Dark" and Color3.fromRGB(70, 70, 70) or Color3.fromRGB(200, 200, 200)
	end
end

-- Minimap için Katil/Şerif ekstra işaret
local function getRoleColor(p)
	if not p.Character then return Color3.fromRGB(0, 170, 255) end
	if p.Character:FindFirstChild("Knife") then
		return Color3.fromRGB(255, 0, 0) -- Kırmızı katil
	elseif p.Character:FindFirstChild("Gun") then
		return Color3.fromRGB(0, 255, 0) -- Yeşil şerif
	elseif p == favoritePlayer then
		return Color3.fromRGB(255, 255, 0) -- Sarı favori
	else
		return Color3.fromRGB(0, 170, 255) -- Mavi normal
	end
end


-- MiniMap İçine Nokta Yerleştirme Fonksiyonu
local function createDot(color)
	local dot = Instance.new("Frame", miniMap)
	dot.Size = UDim2.new(0, 8, 0, 8)
	dot.BackgroundColor3 = color
	dot.AnchorPoint = Vector2.new(0.5, 0.5)
	dot.BorderSizePixel = 0
	dot.Name = "Dot"
	dot.ZIndex = 5
	return dot
end

-- Mini Harita Güncellemesi
RunService.RenderStepped:Connect(function()
	-- Önce tüm eski noktaları temizle
	for _, child in ipairs(miniMap:GetChildren()) do
		if child.Name == "Dot" then
			child:Destroy()
		end
	end

	local mapAreaSize = 1000 -- MM2 harita boyutu yaklaşık
	local localChar = LocalPlayer.Character
	if not localChar or not localChar:FindFirstChild("HumanoidRootPart") then return end
	local localPos = localChar.HumanoidRootPart.Position

	for _, p in ipairs(Players:GetPlayers()) do
		local char = p.Character
		if char and char:FindFirstChild("HumanoidRootPart") then
			local pos = char.HumanoidRootPart.Position
			-- Pozisyonu mini haritaya ölçekle
			local relativePos = Vector3.new(pos.X - localPos.X, 0, pos.Z - localPos.Z)

			local dotX = 0.5 + (relativePos.X / mapAreaSize)
			local dotY = 0.5 + (relativePos.Z / mapAreaSize)

			-- Dot ekle (mini harita alanı içinde sınırla)
			if dotX >= 0 and dotX <= 1 and dotY >= 0 and dotY <= 1 then
				local color =
					p == LocalPlayer and Color3.fromRGB(0, 255, 0) or
					p == favoritePlayer and Color3.fromRGB(255, 255, 0) or
					Color3.fromRGB(0, 170, 255)

				local dot = createDot(color)
				dot.Position = UDim2.new(dotX, 0, dotY, 0)
			end
		end
	end
end)

-- Ana Update Döngüsü
RunService.RenderStepped:Connect(function()
	-- Katil yaklaştıysa sesli uyarı
	local killerNearby = false
	for _, p in ipairs(Players:GetPlayers()) do
		if p.Character and p.Character:FindFirstChild("Knife") then
			local hrp = p.Character:FindFirstChild("HumanoidRootPart")
			local localHRP = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
			if hrp and localHRP then
				local dist = (hrp.Position - localHRP.Position).Magnitude
				if dist < killerNearThreshold then
					killerNearby = true
					break
				end
			end
		end
	end

	if killerNearby and not killerAlertCooldown then
		alertSound:Play()
		killerAlertCooldown = true
		delay(5, function()
			killerAlertCooldown = false
		end)
	end

	-- Favori oyuncu yakın mı?
	checkFavoriteProximity()

	-- Tema renk güncellemesi (tutarlı kalması için)
	local theme = themes[currentTheme] or themes.Dark
	frame.BackgroundColor3 = theme.bg
	title.TextColor3 = theme.fg
	favLabel.TextColor3 = theme.fg
	playerList.TextColor3 = theme.fg
	serverInfoLabel.TextColor3 = theme.fg
	pingLabel.TextColor3 = theme.fg
	reconnectLabel.TextColor3 = theme.fg
	toggleReconnectBtn.TextColor3 = theme.fg
	changeServerBtn.TextColor3 = theme.fg
	favDropdown.BackgroundColor3 = theme.bg
	favDropdown.TextColor3 = theme.fg
	miniMap.BackgroundColor3 = theme.bg
	miniTitle.TextColor3 = theme.fg
end)

